    uriParameters:
      customerId:
        type: string
    type:
      collection.collectionMember:
        collectionMemberResponseType: customer.CustomerResponse
        collectionMemberPrototypeType: customer.Customer
    get:
      displayName: Get customer
      responses:
        200:
          body:
            application/json:
              example: 
                {
                  "identifier": "deb89058-77fb-4e97-8c83-6506c38cacf8",
                  "firstName": "Jason",
                  "lastName": "Bourne",
                  "paymentMethods": [
                    {
