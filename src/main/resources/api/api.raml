#%RAML 1.0
title: Retail Omnichannel Experience API
version: v2.0
baseUri: http://localhost:8081/api

traits:
  geoSearch: !include /exchange_modules/org.mule.templates/retail-geosearch/1.0.0/retail_geosearch.raml

uses:
  common: /exchange_modules/org.mule.templates/retail-common/1.0.1/retail_common.raml
  collection: collections.raml
  payment: /exchange_modules/org.mule.templates/retail-payment/1.0.2/retail_payment.raml
  contact: /exchange_modules/org.mule.templates/retail-contact/1.0.1/retail_contact.raml
  fulfillment: /exchange_modules/org.mule.templates/retail-fulfillment/1.0.2/retail_fulfillment.raml
  taxonomy: /exchange_modules/org.mule.templates/retail-taxonomy/1.0.0/retail_taxonomy.raml
  customer: /exchange_modules/org.mule.templates/retail-customer-types/1.0.2/retail_customer_types.raml
  channel: channel-types.raml
  order: /exchange_modules/org.mule.templates/retail-order-types/1.0.2/retail_order_types.raml
  product: /exchange_modules/org.mule.templates/retail-product/1.0.1/retail_product.raml
  session: /exchange_modules/org.mule.templates/retail-shopping-cart-types/1.0.2/retail_shopping_cart_types.raml
  inventory: /exchange_modules/org.mule.templates/retail-inventory/1.0.1/retail_inventory.raml
  store: /exchange_modules/org.mule.templates/retail-store/1.0.1/retail_store.raml
  partner: /exchange_modules/org.mule.templates/retail-partner/1.0.0/retail_partner.raml

types:

  ContactMethodCollectionResponse:
    type: common.CollectionResponse
    description: |
      Contact method response collection
    properties:
      contactMethods: contact.ContactMethod[]

  ContactMethodResponse:
    type: contact.ContactMethod
    description: |
      Contact method response

  PaymentResponse:
    type: payment.PaymentResponse
    description: |
      Payment response

  ChannelCollectionResponse:
    type: common.CollectionResponse
    description: |
      Channel response collection
    properties:
      channels: channel.Channel[]

  ChannelResponse:
    type: channel.Channel
    description: |
      Channel response

  FulfillmentMethodCollectionResponse:
    type: common.CollectionResponse
    description: |
      Fulfillment method response collection
    properties:
      fulfillmentMethods: fulfillment.Fulfillment[]

  StoreCollectionResponse:
    type: common.CollectionResponse
    description: |
      Store response collection
    properties:
      stores: store.StoreInfo[]

  StoreResponse:
    type: store.StoreInfo
    description: |
      Store response

  PartnerCollectionResponse:
    type: common.CollectionResponse
    description: |
      Partner response collection
    properties:
      partners: partner.PartnerInfo[]

  PartnerResponse:
    type: partner.PartnerInfo
    description: |
      Partner response

  CategoryCollectionResponse:
    type: common.CollectionResponse
    description: |
      category response collection
    properties:
      categories: taxonomy.Category[]

  CategoryResponse:
    type: taxonomy.Category
    description: |
      Category response

  SalesOrderCollectionResponse:
    type: common.CollectionResponse
    description: |
      Sales order response collection
    properties:
      orders: order.SalesOrder[]

  OrderCollectionResponse:
    type: common.CollectionResponse
    description: |
      Order response collection
    properties:
      orders: order.OrderSummary[]

  SalesOrderResponse:
    type: order.SalesOrderHeader
    description: |
      Sales order response

  SalesOrderPaymentMethodResponse:
    type: payment.PaymentMethod
    description: |
      Sales order payment method response

  SalesOrderFulfillmentResponse:
    type: fulfillment.Fulfillment
    description: |
      Sales order fulfillment response

  SalesOrderItemCollectionResponse:
    type: order.SalesOrderItems
    description: |
      Sales order item response collection

  ProductResponse:
    type: product.Product
    description: |
      Product response

  ProductCollectionResponse:
    type: common.CollectionResponse
    description: |
      Product response collection
    properties:
      products: product.Product[]

  FeatureCollectionResponse:
    type: common.CollectionResponse
    description: |
      Feature response collection
    properties:
      features: product.ProductFeature[]

  FeatureResponse:
    type: product.ProductFeature
    description: |
      Feature response
    properties:

  ProductVariantCollectionResponse:
    type: common.CollectionResponse
    description: |
      Product variant response collection
    properties:
      variants: product.ProductVariant[]

  ProductVariantResponse:
    type: product.ProductVariant
    description: |
      Product variant response

  ProductVariantAvailabilityResponse:
    type: inventory.Availability
    description: |
      Product variant availability response
    properties:

  ShoppingCartCollectionResponse:
    type: common.CollectionResponse
    description: |
      Shopping cart response collection
    properties:
      shoppingCarts: session.ShoppingCart[]

  ShoppingCartResponse:
    type: session.ShoppingCart
    description: |
      Shopping cart response

/customers:
  description: |
    Consists of information about customers: name, delivery addresses, payment methods etc.
  type:
    collection.collection:
      collectionResponseType: customer.CustomerCollectionResponse
      collectionMemberPrototypeType: customer.Customer
      collectionMemberResponseType: customer.CustomerResponse
  post:
    displayName: Create customer
    body:
      application/json:
        example: 
          {
                "identifier": "deb89058-77fb-4e97-8c83-6506c38cacf8",
                "firstName": "Jason",
                "lastName": "Bourne",
                "paymentMethods": [
                    {
                      "paymentMethodType": {
                        "medium": "CARD",
                        "issuer": "BANK",
                        "name": "VISA"
                      }
                    }
                ],
                "deliveryAddresses": [
                    {
                      "type": "BILLING",
                      "identifier": "deb89058-77fb-4e97-8c83-6506c3855555",
                      "address": "Main street 122",
                      "city": "San Francisco",
                      "state": "California",
                      "postalCode": "90210",
                      "recipient": "Jason Bourne",
                      "country": "US",
                      "secondaryAddress": null,
                      "mailStopCode": null,
                      "attention": null
                    }
                ]
              }

    responses:
      201:
        body:
          application/json:
            example: 
              {
                "identifier": "deb89058-77fb-4e97-8c83-6506c38cacf8",
                "firstName": "Jason",
                "lastName": "Bourne",
                "paymentMethods": [
                    {
                      "identifier": "deb89058-77fb-4e97-8c83-6506c38c4444",
                      "paymentMethodType": {
                        "medium": "CARD",
                        "issuer": "BANK",
                        "name": "VISA"
                      }
                    }
                ],
                "deliveryAddresses": [
                    {
                      "type": "BILLING",
                      "identifier": "deb89058-77fb-4e97-8c83-6506c3855555",
                      "address": "Main street 122",
                      "city": "San Francisco",
                      "state": "California",
                      "postalCode": "90210",
                      "recipient": "Jason Bourne",
                      "country": "US",
                      "secondaryAddress": null,
                      "mailStopCode": null,
                      "attention": null
                    }
                ]
              }

  get: 
    displayName: Get customers
    is: &queryableCollection [ collection.pageable, collection.sortable ]
    queryParameters:
      firstName: string
      lastName: string
    responses:
      200:
        body:
          application/json:
            example: 
              {
                "_meta": {
                  "messages": []
                },
                "customers": [
                  {
                    "identifier": "deb89058-77fb-4e97-8c83-6506c38cacf8",
                    "firstName": "Jason",
                    "lastName": "Bourne",
                    "paymentMethods": [
                      {
                        "paymentMethodType": {
                          "medium": "CARD",
                          "issuer": "BANK",
                          "name": "VISA"
                        }
                      }
                    ],
                    "deliveryAddresses": [
                      {
                        "identifier": "deb89058-77fb-4e97-8c83-6506c38cacf8",
                        "type": "BILLING",
                        "address": "Main street 122",
                        "city": "San Francisco",
                        "state": "California",
                        "postalCode": "90210",
                        "recipient": "Jason Bourne",
                        "country": "US",
                        "secondaryAddress": "",
                        "mailStopCode": "",
                        "attention": ""
                      }
                    ]
                  }
                ]
              }

