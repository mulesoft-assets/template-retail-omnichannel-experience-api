/channels:
  description: |
    Consists of the most common channels to cover use-cases with WEB,
    mobile and brick-and-mortar(store) based application delivering the Omnichannel user experience.
  type:
    collection.readOnlyCollection:
      collectionResponseType: ChannelCollectionResponse
  get:
    displayName: Get channels
    is: &queryableCollection [ collection.pageable, collection.sortable ]
    queryParameters:
      paymentMedium?: payment.PaymentMediumType
      paymentIssuer?: payment.PaymentIssuerType
      receipt?: fulfillment.ReceiptType
      carrier?: fulfillment.CarrierType
    responses:
      200:
        body:
          application/json:
            example:
              {
                "_meta": {
                  "messages": []
                },
                "channels": [
                  {
                    "channelType": "WEB",
                    "url": "http://www.example.com"
                  },
                  {
                    "channelType": "STORE",
                    "storeNumber": "ae42643a-6d19-483c-82a2-54939d9da13e"
                  },
                  {
                    "channelType": "MOBILE",
                    "name": "iOS"
                  }
                ]
              }

  /{channelId}:
    uriParameters:
      channelId: common.ResourceId
    type:
       collection.readOnlyCollectionMember:
         collectionMemberResponseType: channel.Channel
    get:
      displayName: Get channel
      responses:
        200:
          body:
            application/json:
              example:
                {
                  "channelType": "WEB",
                  "url": "http://www.example.com"
                }
    /paymentMethods:
      description: |
        Channel basically could have different payment methods according to channel type like VISA card, bank transfer, cash.
      type:
        collection.readOnlyCollection:
          collectionResponseType: customer.PaymentMethodCollectionResponse
      get:
        displayName: Get payment methods for channel
        is: *queryableCollection
        queryParameters:
          paymentMedium: payment.PaymentMediumType
          paymentIssuer: payment.PaymentIssuerType
        responses:
          200:
            body:
              application/json:
                example:
                  {
                    "_meta": {
                      "messages": [],
